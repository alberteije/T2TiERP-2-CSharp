    <id name="Id" column="ID">
      <generator class="identity" />
    </id>
    <many-to-one name="Empresa" column="ID_EMPRESA" />
    <property name="Codigo" column="CODIGO" />
    <property name="Nome" column="NOME" />
    <property name="Descricao" column="DESCRICAO" />
    <property name="Tipo" column="TIPO" />
    <property name="Unidade" column="UNIDADE" />
    <property name="BaseCalculo" column="BASE_CALCULO" />
    <property name="Taxa" column="TAXA" />
    <property name="RubricaEsocial" column="RUBRICA_ESOCIAL" />
    <property name="CodIncidenciaPrevidencia" column="COD_INCIDENCIA_PREVIDENCIA" />
    <property name="CodIncidenciaIrrf" column="COD_INCIDENCIA_IRRF" />
    <property name="CodIncidenciaFgts" column="COD_INCIDENCIA_FGTS" />
    <property name="CodIncidenciaSindicato" column="COD_INCIDENCIA_SINDICATO" />
    <property name="RepercuteDsr" column="REPERCUTE_DSR" />
    <property name="Repercute13" column="REPERCUTE_13" />
    <property name="RepercuteFerias" column="REPERCUTE_FERIAS" />
    <property name="RepercuteAviso" column="REPERCUTE_AVISO" />
  </class>
</hibernate-mapping>