<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="ContabilidadeService" namespace="ContabilidadeService.Model" default-lazy="false" xmlns="urn:nhibernate-mapping-2.2">
  <class name="ContabilFechamentoDTO" table="contabil_fechamento" >
    <id name="Id" column="ID">
      <generator class="identity" />
    </id>
    <many-to-one name="Empresa" column="ID_EMPRESA" />
    <property name="DataInicio" column="DATA_INICIO" />
    <property name="DataFim" column="DATA_FIM" />
    <property name="CriterioLancamento" column="CRITERIO_LANCAMENTO" />
  </class>
</hibernate-mapping>