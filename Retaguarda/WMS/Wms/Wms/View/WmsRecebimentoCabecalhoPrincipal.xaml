<UserControl x:Class=".WmsRecebimentoCabecalhoPrincipal"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:view="clr-namespace:"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="700">
    <TabControl>
        <TabItem Name="tabWmsRecebimentoCabecalhoLista" Visibility="Collapsed" IsSelected="{Binding IsListar}">
            <TabItem.Content>
                <DockPanel LastChildFill="True">
                    <view:WmsRecebimentoCabecalhoLista></view:WmsRecebimentoCabecalhoLista>
                </DockPanel>
            </TabItem.Content>
        </TabItem>
        <TabItem Name="tabWmsRecebimentoCabecalho" Visibility="Collapsed" IsSelected="{Binding IsEditar}">
            <view:WmsRecebimentoCabecalho></view:WmsRecebimentoCabecalho>
        </TabItem>
    </TabControl>
</UserControl>
