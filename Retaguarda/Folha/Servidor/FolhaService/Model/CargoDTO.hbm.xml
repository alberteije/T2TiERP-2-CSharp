<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="FolhaService" namespace="FolhaService.Model" default-lazy="false" xmlns="urn:nhibernate-mapping-2.2">
  <class name="CargoDTO" table="cargo" >
    <id name="Id" column="ID">
      <generator class="identity" />
    </id>
    <many-to-one name="Empresa" column="ID_EMPRESA" />
    <property name="Nome" column="NOME" />
    <property name="Descricao" column="DESCRICAO" />
    <property name="Salario" column="SALARIO" />
    <property name="Cbo1994" column="CBO_1994" />
    <property name="Cbo2002" column="CBO_2002" />
  </class>
</hibernate-mapping>